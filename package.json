{
  "name": "TodoApp",
  "version": "0.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node ./index.js",
    "server": "nodemon --inspect ./index.js",
    "client": "npm run start  --prefix ./client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "aws-sdk": "latest",
    "body-parser": "latest",
    "concurrently": "latest",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "latest",
    "express": "latest",
    "http-errors": "~1.6.2",
    "lodash": ">=4.17.21",
    "materialize-css": "^1.0.0-rc.2",
    "node": "latest",
    "nodemon": "latest"
  }
}
